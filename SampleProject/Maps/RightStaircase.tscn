[sub_resource type="GDScript" id="GDScript_ft7fx"]
resource_name = "Secret"
script/source = "# A one-time secret. Once discovered it creates a cell override that shows a newly discovered passage.
extends Area2D

func _ready() -> void:
	# Register this node as storable, but without marker.
	# After store_object() was called, this will automatically queue_free() the object.
	MetSys.register_storable_object(self)

func on_body_entered(body: Node2D) -> void:
	# Create override.
	var override := MetSys.get_cell_override(MetSys.get_current_coords())
	# Change the right wall to passage.
	override.set_border(MetroidvaniaSystem.R, 1)
	# Store the object.
	MetSys.store_object(self)
	# Remove it, so it's not detected again.
	queue_free()
"

